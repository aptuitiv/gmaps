{
    "name": "@aptuitiv/google-maps-display",
    "version": "0.2.0",
    "description": "Library to help with displaying a Google map with markers, overlays, and custom buttons",
    "source": "src/index.ts",
    "main": "dist/index.js",
    "type": "module",
    "scripts": {
        "build": "node scripts/build.js",
        "build:types": "tsc --emitDeclarationOnly --declaration --stripInternal",
        "lint": "eslint --fix --ext .js,.ts .",
